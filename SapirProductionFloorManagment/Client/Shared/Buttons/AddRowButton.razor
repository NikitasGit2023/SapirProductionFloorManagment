
<RadzenButton Variant="Variant.Flat" Shade="Shade.Darker" Text="@Text" Click="@OnClick" Disabled="@Disabled" ButtonStyle="ButtonStyle.Success" />
            

@code {
    [Parameter]
    public EventCallback OnClick { get; set; }


    [Parameter]
    public bool Disabled { get; set; }

    [Parameter]
    public string? Text { get; set; }



}
